{
  "$schema": "http://json.schemastore.org/template",
  "author": "Jules Raffoux",
  "name": "Nuxt SPA with minimal APIs",
  "identity": "Nuxt SPA with minimal APIs",
  "shortName": "nuxt-spa",
  "classifications": ["Web", "SPA", "Nuxt"],
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "sourceName": "Spartacus",
  "sources": [
    {
      "exclude": [
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        ".template.config/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*.lock.json",
        "**/.nuxt",
        "**/node_modules",
        "**/pnpm-lock.yaml",
        "**/proxy-paths.json"
      ]
    }
  ]
}
